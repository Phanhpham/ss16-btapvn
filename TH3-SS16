#include<stdio.h>
int main(){
    FILE *taptin = NULL;
    const int SO_KY_TU_TOI_DA = 1000;
    char chuoi[SO_KY_TU_TOI_DA] = "";
    taptin = fopenfopen("test.txt","r");
    if (taptin !=NULL){
        fgets(chuoi,SO_KY_TU_TOI_DADA,taptin);
        printf("%s",chuoi);
        fclose(taptin);

    }
    return 0;
}